                      <td>@item.Project</td>
                      <td>@item.Country</td>
                      <td>@item.Eligible</td>
                     <td>@item.Fromdate  </td>
                      <td>@item.Todate  </td>
                       <td>@item.Date  </td>
                   <td>@item.Mode</td>
                    <td>@item.Totalamount </td>      
                    <td><a class="btn btn-primary btn-sm" href="ManagerAccount/View/@item.Id">View</a></td>

                     @* <td>
                        <a type="submit" class="btn btn-success btn-sm" asp-action="Approve" asp-controller="ManagerAccount">Accept</a>
                         <a type="submit" class="btn btn-danger btn-sm" asp-action="Decline" asp-controller="ManagerAccount">Decline</a>
                    </td> *@
