<h1>Uploaded Details</h1>
        <table class="table">
            <thead  class="text-info bg-dark">
                <tr>
                    <td>Id</td>
                    <td>Department</td>
                    <td>Project</td>
                    <td>Country</td>
                    <td>Eligible</td>
                    <td>Fromdate</td>
                    <td>Todate</td>
                    <td>Date  </td>   
                    <td>Mode</td>
                    <td>Totalamount </td>
                    <td>Manage</td>
                    
                </tr>
            </thead>
            <tbody>
                @foreach(var item in Model)
                {
                    <tr>
                    <td>@item.Id</td>
