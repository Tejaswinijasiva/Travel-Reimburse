

           

@model Travel_Reimbursement.Models.FileModel

 <!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Travel_Reimbursement</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Travel_Reimbursement.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.3.0/css/all.min.css" integrity="sha512-SzlrxWUlpfuzQ+pcUCosxcglQRNAq/DZjVsC0lE40xsADsfeQoEypE+enwcOiGjk/bSuGGKHEyjSoQ1zVisanQ==" crossorigin="anonymous" referrerpolicy="no-referrer" />
</head>
<body>
     <header class="position-sticky top-0">
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            
            <div class="container-fluid">
                <a class="navbar-brand fw-bold text-blue" asp-area="" asp-controller="Home" asp-action="Index">Travel_Reimbursement</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                 <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav">
                        @* <li class="nav-item">
                            <a class="nav-link fw-bold text-blue" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li> *@
                         <li class="nav-item">
                            <a class="nav-link fw-bold text-blue" asp-area="" asp-controller="Details" asp-action="Index">Details</a>
                        </li>
                         <li class="nav-item">
                            <a class="nav-link fw-bold text-blue" asp-area="" asp-controller="File" asp-action="IndexFile">FileUpload</a>
                        </li>
                    </ul>
                </div>
                <div class="collapse navbar-collapse d-sm-inline-flex justify-content-end" id="navbarNav">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link fw-bold text-blue" asp-area="" asp-controller="Accounts" asp-action="Logout">Logout</a>
                        </li>
                         
                    </ul>
                </div>
            </div>
        </nav>
     </header>




<h4>File Upload</h4>
<hr />
<div class="row">
    <div class="col-4 d-flex justify-content-center">
       
    <div class="col-md-4">
        <form asp-action="Fileupload" asp-area="File" method="post" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Title" class="Control-label"></label>
                  @* @Html.TextBoxFor(m => m.Title, new { @class = "form-control" })
            @Html.ValidationMessageFor(m => m.Title, "",new { @class ="text-danger"})
              <span class="text-danger">@TempData["errorTitle"]</span> *@
                <input asp-for="Title" class="form-control"/>
                <span asp-validation-for="Title" class="text-danger"></span>
            </div><br>
             <div class="form-group">
                <label asp-for="ImageFile" class="Control-label"></label>
                  <input asp-for="ImageFile" accept="image/*"/>
            @* @Html.ValidationMessageFor(m => m.ImageFile, "",new { @class ="text-danger"})
              <span class="text-danger">@TempData["errorImageFile"]</span> *@
              <span asp-validation-for="ImageFile" class="text-danger"></span> 
            </div><br>
            <div class="form-group">
                <input type="submit" value="Fileupload" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
</div>
<div>
    <a asp-action="IndexFile">History</a>
</div>
<br>
<br>
<br>
<footer class="border-top text-white fixed-bottom ">
        <div class="container mt-2">
         <div class="row">
            <div class="col-sm-4 col-md-4 col-lg-4 col-xl-4">
                <h4>Travel Reimbursement</h4>
                <pre>Security</pre>
                <pre>Protection</pre>
                <pre>Saved</pre>
             
            </div>
            <div class="col-sm-4 col-md-4 col-lg-4 col-xl-4">
                <h4>Contact Us</h4>
                <div class="mt-3 mb-2">
                    <span><i class="fa-brands fa-instagram fs-5"></i></span>
                    <span class="fs-6 mx-1"><a href="https://instagram.com/aspire_systems?igshid=YmMyMTA2M2Y= "> Instagram</a></span>
                </div>
                 <div class="mb-2">
                    <span><i class="fa-brands fa-youtube fs-5"></i></span>
                    <span class="fs-6 mx-1"><a href="https://youtube.com/Aspiresys">YouTube</a></span>
                </div>
                 <div class="mb-2"> 
                    <span><i class="fa-brands fa-facebook fs-5"></i></span>
                    <span class="fs-6 mx-1"><a href="https://www.facebook.com/AspireSystems?mibextid=ZbWKwL">Facebook</a></span>
                </div> 
                 <div class="mb-2">
                    <span><i class="fa-regular fa-linkedin fs-5"></i></span>
                    <span class="fs-6 mx-1"><a href="https://www.linkedin.com/company/aspire-systems/">LinkedIn</a></span>
                </div>
                @* <div class="mb-2">
                    <span><i class="fa-regular fa-envelope fs-5"></i></span>
                    <span class="fs-6 mx-1"><a href="mailto:aspiresys@gmail.com"> Mail</a></span>
                </div> *@
            </div>
            <div class="col-sm-4 col-md-4 col-lg-4 col-xl-4">
                <h4>Address</h4>
                <pre>Aspire Systems</pre>
                <pre>Chennai, Siruseri, TamilNadu, India</pre>    
            </div>
         </div>
       <!--  <div>
            <h3  class="text-center">Powered By Gayathri</h3>
         </div>-->
          </div>
    </footer> 
</body>
</html>

@section Scripts{
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
